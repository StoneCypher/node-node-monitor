{

    "comma-dangle"                  : ["error", "never"],
    "no-cond-assign"                : 0,
    "no-console"                    : 0,
    "no-constant-condition"         : 1,
    "no-control-regex"              : 1,
    "no-debugger"                   : 1,
    "no-dupe-keys"                  : 2,
    "no-duplicate-case"             : 2,
    "no-empty"                      : 1,
    "no-empty-character-class"      : 2,
    "no-ex-assign"                  : 2,
    "no-extra-boolean-cast"         : 1,
    "no-extra-parens"               : 0,
    "no-extra-semi"                 : 2,
    "no-func-assign"                : 2,
    "no-inner-declarations"         : 1,
    "no-invalid-regexp"             : 2,
    "no-irregular-whitespace"       : 2,
    "no-negated-in-lhs"             : 2,
    "no-obj-calls"                  : 2,
    "no-regex-spaces"               : 1,
    "no-sparse-arrays"              : 1,
    "no-unexpected-multiline"       : 1,
    "no-unreachable"                : 2,
    "use-isnan"                     : 2,
    "valid-jsdoc"                   : 1,
    "valid-typeof"                  : 2,

    "accessor-pairs"                : 2,
    "array-callback-return"         : 2,
    "block-scoped-var"              : 2,
    "complexity"                    : ["warn", 30],
    "consistent-return"             : 2,
    "curly"                         : 2,
    "default-case"                  : 2,
    "dot-location"                  : ["warn", "property"],
    "dot-notation"                  : 1,
    "eqeqeq"                        : 2,
    "guard-for-in"                  : 1,
    "no-alert"                      : 1,
    "no-caller"                     : 2,
    "no-case-declarations"          : 2,
    "no-div-regex"                  : 1,
    "no-else-return"                : 0,
    "no-empty-function"             : ["warn", {"allow": ["constructors"]}],
    "no-empty-pattern"              : 1,
    "no-eq-null"                    : 2,
    "no-eval"                       : 2,
    "no-extend-native"              : 2,
    "no-extra-bind"                 : 1,
    "no-extra-label"                : 2,
    "no-fallthrough"                : 2,
    "no-floating-decimal"           : 2,
    "no-implicit-coercion"          : 2,
    "no-implicit-globals"           : 1,
    "no-implied-eval"               : 2,
    "no-invalid-this"               : 1,
    "no-iterator"                   : 2,
    "no-labels"                     : 2,
    "no-lone-blocks"                : 1,
    "no-loop-func"                  : 1,
    "no-magic-numbers"              : 1,
    "no-multi-spaces"               : 0,
    "no-multi-str"                  : 2,
    "no-native-reassign"            : 2,
    "no-new"                        : 2,
    "no-new-func"                   : 2,
    "no-new-wrappers"               : 2,
    "no-octal"                      : 2,
    "no-octal-escape"               : 1,
    "no-param-reassign"             : 2,
    "no-proto"                      : 2,
    "no-redeclare"                  : 2,
    "no-return-assign"              : 0,
    "no-script-url"                 : 2,
    "no-self-assign"                : 2,
    "no-self-compare"               : 2,
    "no-sequences"                  : 0,
    "no-throw-literal"              : 1,
    "no-unmodified-loop-condition"  : 2,
    "no-unused-expressions"         : 1,
    "no-unused-labels"              : 2,
    "no-useless-call"               : 2,
    "no-useless-concat"             : 2,
    "no-useless-escape"             : 2,
    "no-void"                       : 2,
    "no-warning-comments"           : [1, {"terms":["todo","fixme","whargarbl","comeback","tolearn","todiscuss"],"location":"anywhere"}],
    "no-with"                       : 2,
    "radix"                         : 2,
    "vars-on-top"                   : 1,
    "wrap-iife"                     : 0,
    "yoda"                          : 1,

    "strict"                        : 2,

    "init-declarations"             : 0,
    "no-catch-shadow"               : 2,
    "no-delete-var"                 : 2,
    "no-label-var"                  : 1,
    "no-restricted-globals"         : 1,
    "no-shadow"                     : 2,
    "no-shadow-restricted-names"    : 2,
    "no-undef"                      : 0,
    "no-undef-init"                 : 0,
    "no-undefined"                  : 0,
    "no-unused-vars"                : ["warn", { "args": "all", "varsIgnorePattern": "^_.+", "argsIgnorePattern": "^_.+" }],
    "no-use-before-define"          : 2,

    "callback-return"               : 2,
    "global-require"                : 2,
    "handle-callback-err"           : 1,
    "no-mixed-requires"             : 1,
    "no-new-require"                : 1,
    "no-path-concat"                : 2,
    "no-process-env"                : 0,
    "no-process-exit"               : 1,
    "no-restricted-modules"         : 0,
    "no-sync"                       : 0,

    "array-bracket-spacing"         : 0,
    "block-spacing"                 : ["warn", "always"],
    "brace-style"                   : 0,
    "camelcase"                     : 0,
    "comma-spacing"                 : 0,
    "comma-style"                   : 0,
    "computed-property-spacing"     : 0,
    "consistent-this"               : ["error", "outerThis"],
    "eol-last"                      : 1,
    "func-names"                    : 0,
    "func-style"                    : 0,
    "id-blacklist"                  : ["error", "whargarbl", "todo"],
    "id-length"                     : 0,
    "id-match"                      : 0,
    "indent"                        : 0,
    "jsx-quotes"                    : 2,
    "key-spacing"                   : [1, {"afterColon": true, "align": "colon"}],
    "keyword-spacing"               : 2,
    "linebreak-style"               : 0,
    "lines-around-comment"          : ["warn", { "beforeBlockComment": true }],
    "max-depth"                     : ["warn", 5],
    "max-len"                       : 0,
    "max-nested-callbacks"          : ["warn", 5],
    "max-params"                    : ["warn", 10],
    "max-statements"                : 0,
    "max-statements-per-line"       : 0,
    "new-cap"                       : 1,
    "new-parens"                    : 2,
    "newline-after-var"             : "warn",
    "newline-before-return"         : 0,
    "newline-per-chained-call"      : "warn",
    "no-array-constructor"          : 2,
    "no-bitwise"                    : 0,
    "no-lonely-if"                  : 2,
    "no-mixed-spaces-and-tabs"      : 1,
    "no-multiple-empty-lines"       : 0,
    "no-negated-condition"          : 0,
    "no-nested-ternary"             : 0,
    "no-new-object"                 : 1,
    "no-plusplus"                   : 0,
    "no-restricted-syntax"          : 0,
    "no-spaced-func"                : "warn",
    "no-ternary"                    : 0,
    "no-trailing-spaces"            : 1,
    "no-underscore-dangle"          : 1,
    "no-unneeded-ternary"           : 1,
    "no-whitespace-before-property" : 1,
    "object-curly-spacing"          : 0,
    "one-var"                       : 0,
    "one-var-declaration-per-line"  : "warn",
    "operator-assignment"           : "warn",
    "operator-linebreak"            : ["warn", "before"],
    "padded-blocks"                 : 0,
    "quotes"                        : [1, "single"],
    "quote-props"                   : 0,
    "require-jsdoc"                 : ["warn", {"require": {"FunctionDeclaration": true, "MethodDefinition": true, "ClassDeclaration": true}}],
    "semi"                          : [2, "always"],
    "semi-spacing"                  : ["warn", {"before": false, "after": true}],
    "sort-imports"                  : 0,
    "sort-vars"                     : 0,
    "space-before-blocks"           : [2, "always"],
    "space-before-function-paren"   : ["warn", "never"],
    "space-in-parens"               : ["warn", "never"],
    "space-infix-ops"               : "warn",
    "space-unary-ops"               : ["warn", { "words": true, "nonwords": false }],
    "spaced-comment"                : ["warn", "always", { "block": { "exceptions": ["*"] }}],
    "wrap-regex"                    : 0,

    "arrow-body-style"              : ["warn", "as-needed"],
    "arrow-parens"                  : ["warn", "as-needed"],
    "arrow-spacing"                 : ["warn", {"before":true, "after":true}],
    "constructor-super"             : "error",
    "generator-star-spacing"        : ["warn", "both"],
    "no-class-assign"               : "error",
    "no-confusing-arrow"            : 0,
    "no-const-assign"               : "error",
    "no-dupe-class-members"         : "error",
    "no-duplicate-imports"          : 0,
    "no-new-symbol"                 : "error",
    "no-restricted-imports"         : 0,
    "no-this-before-super"          : "error",
    "no-useless-constructor"        : "warn",
    "no-var"                        : "warn",
    "object-shorthand"              : "warn",
    "prefer-arrow-callback"         : "warn",
    "prefer-const"                  : "warn",
    "prefer-reflect"                : 0,
    "prefer-rest-params"            : "warn",
    "prefer-spread"                 : "warn",
    "prefer-template"               : "warn",
    "require-yield"                 : "error",
    "template-curly-spacing"        : ["warn", "never"],
    "yield-star-spacing"            : ["warn", "both"],

    "react/jsx-uses-react"          : "error",
    "react/jsx-uses-vars"           : "error"

}